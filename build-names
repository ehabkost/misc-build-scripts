x86-kvm-build	--enable-kvm --target-list=x86_64-softmmu,i386-softmmu,x86_64-linux-user,i386-linux-user
x86-nokvm-build	--disable-kvm --target-list=x86_64-softmmu,i386-softmmu,x86_64-linux-user,i386-linux-user
all-kvm-build	--enable-kvm
all-nokvm-build	--disable-kvm
some-win-build	--cross-prefix=i686-w64-mingw32- --target-list=x86_64-softmmu,i386-softmmu,mips-softmmu,s390x-softmmu,ppc-softmmu
